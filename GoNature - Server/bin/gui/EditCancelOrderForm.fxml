<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.EditCancelOrderController">
   <children>
      <AnchorPane prefHeight="419.0" prefWidth="700.0">
         <children>
            <TableView fx:id="orderTable" layoutX="10.0" layoutY="81.0" prefHeight="271.0" prefWidth="680.0">
              <columns>
                <TableColumn fx:id="colParkName" prefWidth="242.0" text="Park Name" />
                <TableColumn fx:id="colDate" prefWidth="128.0" text="Date" />
                  <TableColumn fx:id="colVisitorsNum" prefWidth="98.0" text="#Visitors" />
                  <TableColumn fx:id="colEmail" prefWidth="211.0" text="Email" />
              </columns>
            </TableView>
            <Button fx:id="btnShowCurrOrder" layoutX="26.0" layoutY="33.0" mnemonicParsing="false" onAction="#showOrders" text="Show current orders" />
            <Button fx:id="btnEditOrder" layoutX="454.0" layoutY="367.0" mnemonicParsing="false" onAction="#getEditOrderBtn" prefHeight="31.0" prefWidth="94.0" text="Edit order" />
            <Button fx:id="btnCancelOrder" layoutX="569.0" layoutY="367.0" mnemonicParsing="false" text="Cancel order" />
            <Button fx:id="btnBack" layoutX="52.0" layoutY="367.0" mnemonicParsing="false" onAction="#getBackBtn" text="Back" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
